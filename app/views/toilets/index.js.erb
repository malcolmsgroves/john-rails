console.log('<%= params[:page] %>');

$('#toilets').append('<%= j render @toilets %>');
$('.loading').hide();

<% if @toilets.next_page %>
  $('.pagination').replaceWith('<%= j will_paginate @toilets %>');
  setInfiniteScroll();
<% else %>
  console.log('last page');
  $(window).off('scroll');
  $('.pagination').remove();
<% end %>
